
@{
    ViewBag.Title = "msgpass";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<br />
<br />
<br />
<br />
<br />
<br />
<br />

<h1>Message Box</h1>
<hr style="border:5px solid #1e222d"/>

    <table class="table table-hover">

        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Message</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var i in ViewBag.msgList)
            {
                <tr>
                    <td>@i.feedName</td>
                    <td>@i.feedEmail</td>
                    <td>@i.feedMessage</td>
                </tr>

            }
        </tbody>
    </table>
